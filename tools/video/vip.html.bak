//获取具体接口地址
function getUrl(url){
	
	if(url=='')return;
	//获取站点对应的api列表对象
	var site_apis = window['_'+getSite(url)];

	if(site_apis && site_apis.length>0){
		//if(location.search.indexOf('auto_play_next=true')>0 && site_apis.auto_play_next.length>0){
		//	//使用自动播放下一级的接口
		//	site_apis = site_apis.auto_play_next;
		//}
		//随机获取其中一个地址
		return site_apis[parseInt(Math.random()*site_apis.length)].url+url;
	}else{
		if(!window.default_apis||window.default_apis.length==0){
			//默认api
			window.default_apis = jiexi_apis.filter(function(i,e){
				return i.sites.length>5
			});
			//517解析:爱奇艺、优酷、乐视、芒果、搜狐、PPTV、华数tv、1905、咪咕、m3u8，MP4
			default_apis.push('http://cn.bjbanshan.cn/jx.php?url=');
		}
		return default_apis[randomIdx(default_apis.length)].url + url;
	}
}

//解析通过实时接口播放：from Ckplayer论坛 1个月内
function byTempApi(video, opennew=false){
	if(!window.tmp_apis || window.tmp_apis.length==0){
		var url='https://bbs.ckplayer.com/search.php?mod=forum&searchid=58&orderby=lastpost&ascdesc=desc&searchsubmit=yes&kw=全网+解析+无广告+接口+http';
		var data='method=0&host='+encodeURIComponent(url)+'&hideRAW=';

		fetch("http://tool.chinaz.com/Tools/httptest.aspx",{
			method:"post",
			headers: {
				"Content-Type": "application/x-www-form-urlencoded;charset=UTF-8"
			},
			body: data //向服务端传入json数据
		}).then(function (res) {
			res.text().then(function (value) {				
				var pattern = /[接口|地址].*?(\/\/[\w|\.]+(:\d+)?\/.*?\?\w+=)/g;
				var groups = value.match(pattern);
				window.tmp_apis = [];
				for(var group of groups){
					let _url =/[接口|地址].*?(\/\/[\w|\.]+(:\d+)?\/.*?\?\w+=)/.exec(group)[1];
					if(_url.indexOf('stat')==-1 && !tmp_apis.includes(_url)){
						tmp_apis.push(_url)
					}
				}
				tmp_apis.push('http://cn.bjbanshan.cn/jx.php?url=');
				if(opennew){
					window.open(tmp_apis[randomIdx(tmp_apis.length)] + url, '_blank');
				}else{
					$('#player').attr('src',tmp_apis[randomIdx(tmp_apis.length)] + url);
				}
			})
		})
	}else{
		if(opennew){
			window.open(tmp_apis[randomIdx(tmp_apis.length)] + url, '_blank');
		}else{
			$('#player').attr('src',tmp_apis[randomIdx(tmp_apis.length)] + url);
		}
	}
}

//解析视频
function jiexi(opennew=false) {
	if(window.using_tmp_api){
		byTempApi($("#url").val(), opennew);
	}else{
		var url = getUrl($("#url").val());
		if(url && url!=''){
			if(opennew){window.open(url, '_blank');
			}else{
				$('#player').attr('src',url);
			}
		}
	}
}

